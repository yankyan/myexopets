<p id="notice"><%= notice %></p>

<h1>Molts</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @molts.each do |molt| %>
      <tr>
        <td><%= molt.date %></td>
        <td><%= link_to 'Show', molt %></td>
        <td><%= link_to 'Edit', edit_molt_path(molt) %></td>
        <td><%= link_to 'Destroy', molt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Molt', new_molt_path %>
